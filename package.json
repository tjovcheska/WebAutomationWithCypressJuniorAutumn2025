{
  "name": "web-automation-cypress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -rf cypress/reports && rm -rf cypress/mochawesome-report",
    "pretest": "npm run clean:reports",
    "test": "npx cypress run",
    "posttest": "npm run merge-reports && npm run generate-report",
    "merge-reports": "npx mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mocha/finalMochawesomeReport.json",
    "generate-report": "npx marge cypress/reports/mocha/finalMochawesomeReport.json --reportDir cypress/reports --reportTitle 'Cypress Tests'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^15.3.0",
    "cypress-multi-reporters": "^2.0.5",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0"
  }
}
